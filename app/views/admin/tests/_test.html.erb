  <tr>
    <th style='text-align: left;'>
      <%= link_to test.title, admin_test_questions_path(test), class:'nav-link test-title', data: { test_id: test.id } %>
      <%= render 'form_inline', test: test == @test ? @test : test  %>
      </th>
    <th><%= test.level %></th>
    <th><%= test.questions.length %></th>
    <th><%= link_to t('.edit'), edit_admin_test_path(test), class: 'btn btn-warning' %></th>
    <th><%= link_to t('.fast_edit'), nil, class: 'btn btn-primary form-inline-link', data: {test_id: test.id } %></th>
    <th><%= button_to t('.delete'), admin_test_path(test), method: :delete,
                                                                       data: { confirm: t('.confirm') },
                                                                       class: 'btn btn-danger' %></th>
  </tr>
